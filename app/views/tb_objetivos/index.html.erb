<p id="notice"><%= notice %></p>

<h1>Tb Objetivos</h1>

<table>
  <thead>
    <tr>
      <th>Codigo</th>
      <th>Descripcion</th>
      <th>Tipo objetivo</th>
      <th>Codigo organizacion</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tb_objetivos.each do |tb_objetivo| %>
      <tr>
        <td><%= tb_objetivo.codigo %></td>
        <td><%= tb_objetivo.descripcion %></td>
        <td><%= tb_objetivo.tipo_objetivo %></td>
        <td><%= tb_objetivo.codigo_organizacion %></td>
        <td><%= tb_objetivo.status %></td>
        <td><%= link_to 'Show', tb_objetivo %></td>
        <td><%= link_to 'Edit', edit_tb_objetivo_path(tb_objetivo) %></td>
        <td><%= link_to 'Destroy', tb_objetivo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tb Objetivo', new_tb_objetivo_path %>
