 <section id="contact" class="team">
        <div class="container">
            <div class="row m-b-lg">
                <div class="col-lg-12 text-center">
                    <div class="navy-line"></div>
                    <h1>Contactanos</h1>
                    <p class="lead">Porque la belleza también es salud.</p>
                </div>
            </div>
        </div>
  
        <div class="container">
            <div class=" image" style=" float:left; margin-left:35px;" style="width:680px"> <%= image_tag "contacto.jpg"%>
                <address style=""><br>
                <strong><span class="navy">Spinetti Spa Gym.</span></strong><br/>
                Carrera 16, entre Calle 30 y 31, Nº. 30-68<br/>
                Barquisimeto, Lara.<br/>
                <abbr title="Telefonos">P:</abbr> (0251)232.0855 (0251)232.7795 (0251)718.8049
                </address>
      </div>
    
            <div class="container" style=" float:left; width:350px" >
           
                <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3928.4436198200324!2d-69.32335188475284!3d10.062689292808207!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x8e876710e459444f%3A0xad224ca3c55aef8!2sGrupo+M%C3%A9dico+Spinetti!5e0!3m2!1ses-419!2sve!4v1485975400878" width="400" height="300" frameborder="0" style="border:0" allowfullscreen></iframe>
      
            </div>
        </div>

      

        <div class="container citas">
          
            <div class="stepwizard col-lg-8 col-lg-offset-2">
               <div class="stepwizard-row setup-panel">
               <div class="stepwizard-step">
                  <a href="#step-1" type="button" class="btn btn-primary btn-circle">1</a>
                  <p>Datos personales</p>
                </div>
                <div class="stepwizard-step">
                  <a href="#step-2" type="button" class="btn btn-default btn-circle" disabled="disabled">2</a>
                  <p>Mensaje</p>
                </div>
            </div>
          </div>
  
            <form role="form" action="" method="post">
              <div class="row setup-content" id="step-1">
                <div class="col-xs-12">
                  <div class="col-md-12">
                    <h3> Datos Personales</h3>
                     <div class="col-md-6">
                    <div class="form-group">
                      <label class="control-label">Nombre</label>
                      <input  maxlength="100" type="text" class="form-control" placeholder="Ingrese su nombre"  />
                    </div> </div>
                     <div class="col-md-6">
                    <div class="form-group">
                      <label class="control-label">Apellido</label>
                      <input maxlength="100" type="text" class="form-control" placeholder="Ingrese su apellido" />
                    </div></div>
                      <div class="col-md-6">
                    <div class="form-group">
                      <label class="control-label">Cedula</label>
                      <input maxlength="100" type="text" class="form-control" placeholder="Ingrese su cedula" />
                    </div></div>
                     <div class="col-md-6">
                    <div class="form-group">
                      <label class="control-label">Correo</label>
                      <input maxlength="100" type="text" required="required" class="form-control" placeholder="Ingrese su correo" />
                    </div></div>
                      <div class="col-md-6">
                    <div class="form-group">
                      <label class="control-label">Telefono</label>
                      <input maxlength="100" type="text" class="form-control" placeholder="Ingrese su telefono" />
                    </div></div>
                    <div class="col-md-6">
                    <div class="form-group">
                      <label class="control-label">Estado</label>
                                   <select maxlength="150" name="servicio" class="form-control"> 
                                        <option >Lara</option>
                                      <option >Carabobo</option>
                                        <option >Yaritagua</option>
                                    </select>
                    </div></div>
                    <div class="col-md-6">
                    <div class="form-group">
                      <label class="control-label">Ciudad</label>
                                   <select maxlength="150" name="servicio" class="form-control"> 
                                        <option >Barquisimeto</option>
                                      <option >Cabudare</option>
                                        <option >La mora</option>
                                    </select>
                    </div></div>
                    


                     <div class="col-md-6 col-md-offset-6">
                    <button class="btn btn-primary nextBtn btn-lg pull-right" type="button" >Siguiente</button>
                  </div>
                  </div>
                </div>
              </div>
              <div class="row setup-content" id="step-2">
                <div class="col-xs-12 ">
                  <div class="col-md-12">
                    <h3> Mensaje</h3>
                    <div class="col-md-4">
                        <div class="form-group">
                                            <label>Motivo del mensaje</label>
                                            <select type="text" name="motivo" class="form-control" required>
                                                <option>Duda</option>
                                                <option>Sugerencia</option>
                                                <option>Reclamo</option>
                                            </select>
                
                                        </div>
                    </div>
                   <div class="col-md-4">
                        <div class="form-group">
                                            <label>Relacionado con</label>
                                            <select type="text" name="motivo" class="form-control" required>
                                                <option>Servicio</option>
                                                <option>Instalación</option>
                                                <option>Atención</option>
                                                <option>Organización</option>
                                            </select>
                
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="form-group">
                                            <label>Especialmente de</label>
                                            <select type="text" name="motivo" class="form-control" required>
                                                <option>Sauna</option>
                                                <option>Depilación</option>
                                                <option>Cavitación</option>
                                                <option>Limpieza Facial</option>
                                            </select>
                
                                        </div>
                    </div>
                <div class="col-sm-12">
                   <div class="form-group">
                                            <label>Mensaje *</label>
                                            <textarea name="message" id="message" required class="form-control" rows="8"></textarea>
                    </div>  
                </div>
                <div class="col-md-6 col-md-offset-6">
                    <button class="btn btn-primary nextBtn btn-lg pull-right" type="submit" >Enviar</button>
                    </div>
                  </div>
                </div>
              </div>
           
            </form>
            
          </div>
</section>
<%= render '/partials/footer' %>